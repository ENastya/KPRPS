<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/task/List.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>taskList</from-outcome>
            <to-view-id>/task/List.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>View</from-outcome>
            <to-view-id>/task/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/task/View.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Start</from-outcome>
            <to-view-id>/process/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/user/View.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/user/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/project/View.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/project/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/task/ActiveList.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Start</from-outcome>
            <to-view-id>/process/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/process/View.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>TaskView</from-outcome>
            <to-view-id>/task/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/user/Create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/user/Errorpage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
